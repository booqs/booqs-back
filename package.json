{
  "name": "booqs-back",
  "private": true,
  "version": "0.1.0",
  "description": "Booqs backend",
  "engines": {
    "node": "18.x"
  },
  "main": "dist/server/index.js",
  "types": "dist/server/index.d.ts",
  "bin": {
    "booqs": "dist/cli/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint server/**/*.ts",
    "build": "tsc && npm run lint",
    "start": "node dist/server/index.js",
    "dev": "npm run https",
    "http": "cross-env NODE_ENV=development nodemon server/index.ts",
    "https": "cross-env NODE_ENV=development PORT=4001 nodemon server/index.ts & npx local-ssl-proxy --source 4000 --target 4001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/booqs/booqs-back"
  },
  "keywords": [
    "booqs",
    "backend"
  ],
  "author": "Anton Shakirzianov",
  "bugs": {
    "url": "https://github.com/booqs/booqs-back/issues"
  },
  "homepage": "https://github.com/booqs/booqs-back",
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/cors": "^2.8.13",
    "@types/css": "0.0.31",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@aws-sdk/client-s3": "^3.379.1",
    "@types/htmlparser2": "^3.10.1",
    "@types/sharp": "^0.31.1",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "css": "^2.2.4",
    "css-select": "^5.1.0",
    "domutils": "^3.0.1",
    "dotenv": "^8.2.0",
    "epub2": "^3.0.1",
    "express": "^4.18.2",
    "graphql": "^16.7.1",
    "graphql-tools": "^9.0.0",
    "graphql-type-json": "^0.3.2",
    "htmlparser2": "^8.0.1",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.19",
    "mongoose": "^7.4.1",
    "sharp": "^0.31.3",
    "specificity": "^0.4.1"
  }
}
